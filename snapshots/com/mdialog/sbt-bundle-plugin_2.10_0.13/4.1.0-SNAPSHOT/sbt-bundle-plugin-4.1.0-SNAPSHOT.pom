<?xml version='1.0' encoding='UTF-8'?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0">
    <modelVersion>4.0.0</modelVersion>
    <groupId>com.mdialog</groupId>
    <artifactId>sbt-bundle-plugin</artifactId>
    <packaging>jar</packaging>
    <description>sbt plugin to create a self-contained deployable application bundle</description>
    <version>4.1.0-SNAPSHOT</version>
    <name>sbt-bundle-plugin</name>
    <organization>
        <name>com.mdialog</name>
    </organization>
    <properties>
